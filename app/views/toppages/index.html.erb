<% if logged_in? %>
  
    <div class="col-sm-8">
      <%= render "posts/posts", posts: @posts %>
    </div>
  </div>
<% else %>
  <div class="jumbotron jumbotron-fluid">
      <div class="text-center">
        <h1>FavPhoto</h1>
        <p>大好きな写真を投稿して共有しよう</p>
        <%= link_to '新規登録', signup_path, class: 'btn btn-lg btn-primary' %>
      </div>
  </div>
<% end %>

